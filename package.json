{
  "name": "mongo-aggregate-transform",
  "version": "1.0.0",
  "description": "Special transforms to handle type casting in mongo aggregate pipes.",
  "main": "lib/",
  "scripts": {
    "lint": "semistandard --fix",
    "mocha": "mocha --opts mocha.opts",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha -- --opts mocha.opts",
    "test": "npm run lint && npm run coverage"
  },
  "semistandard": {
    "env": [
      "mocha"
    ]
  },
  "engines": {
    "node": ">= 6"
  },
  "directories": {
    "lib": "lib"
  },
  "keywords": [
    "mongo",
    "aggregate",
    "transform",
    "cast",
    "object",
    "string"
  ],
  "author": "Bryan Nielsen <bnielsen1965@gmail.com> (https://github.com/bnielsen1965)",
  "license": "MIT",
  "devDependencies": {
    "istanbul": "^0.4.5",
    "mocha": "^5.0.0",
    "semistandard": "^12.0.0"
  },
  "dependencies": {
    "mongodb": "^3.0.1"
  }
}
